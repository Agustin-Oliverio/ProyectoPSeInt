Proceso sin_titulo
	Definir A,B como entera;
	Escribir "Ingrese los goles del primer equipo";
	Leer A;
	Escribir "Ingrese los goles del segundo equipo";
	Leer B;
	Si A>B Entonces
		Escribir "el primer equipo ganó el partido con ", A, " goles";
		FinSi
		Si A<B Entonces
		Escribir "el segundo equipo ganó el partido con ", B, " goles";
	FinSi
	Si A==B Entonces
		Escribir "Hubo empate entre los dos equipos";
		
	FinSi
FinProceso
